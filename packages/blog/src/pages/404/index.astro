---
import ErrorLayout from '@/layouts/ErrorLayout/ErrorLayout.astro';
import lottie404light from './_lottie-404-light.json';
import lottie404dark from './_lottie-404-dark.json';
import { ThemedLottie } from '@/components/ThemedLottie';
---

<ErrorLayout title="404 Not Found">
  <ThemedLottie
    autoplay
    keepLastFrame
    src={lottie404light}
    srcDark={lottie404dark}
    client:load
  />
</ErrorLayout>
